{
	"name": "surrealdb-orm-enhanced",
	"version": "3.0.0",
	"description": "Улучшенный типизированный ORM для SurrealDB с валидацией, кэшированием, безопасностью и мониторингом",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"docs",
		"examples",
		"README.md"
	],
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"clean": "rm -rf dist",
		"prepublishOnly": "npm run clean && npm run build",
		"test": "vitest",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest --coverage",
		"test:integration": "vitest --config vitest.integration.config.ts",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --ext .ts --fix",
		"format": "prettier --write src/**/*.ts",
		"docs": "typedoc src --out docs/api",
		"examples": "tsx examples/enhanced-examples.ts"
	},
	"keywords": [
		"surrealdb",
		"orm",
		"typescript",
		"database",
		"schema",
		"migrations",
		"validation",
		"caching",
		"security",
		"monitoring",
		"performance",
		"connection-pool",
		"rate-limiting",
		"audit",
		"nosql"
	],
	"author": "FrankFMY",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/FrankFMY/surrealdb-orm-enhanced.git"
	},
	"bugs": {
		"url": "https://github.com/FrankFMY/surrealdb-orm-enhanced/issues"
	},
	"homepage": "https://github.com/FrankFMY/surrealdb-orm-enhanced#readme",
	"dependencies": {
		"ws": "^8.14.2"
	},
	"devDependencies": {
		"@types/node": "^20.10.0",
		"@types/ws": "^8.5.10",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitest/coverage-v8": "^1.0.0",
		"eslint": "^8.0.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"prettier": "^3.0.0",
		"typescript": "^5.3.0",
		"tsx": "^4.6.0",
		"typedoc": "^0.25.0",
		"vitest": "^1.0.0"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"peerDependencies": {
		"typescript": ">=5.0.0"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./enhanced": {
			"import": "./dist/orm-enhanced.js",
			"require": "./dist/orm-enhanced.js",
			"types": "./dist/orm-enhanced.d.ts"
		},
		"./validation": {
			"import": "./dist/types/validation.js",
			"require": "./dist/types/validation.js",
			"types": "./dist/types/validation.d.ts"
		},
		"./cache": {
			"import": "./dist/cache/index.js",
			"require": "./dist/cache/index.js",
			"types": "./dist/cache/index.d.ts"
		},
		"./errors": {
			"import": "./dist/errors/index.js",
			"require": "./dist/errors/index.js",
			"types": "./dist/errors/index.d.ts"
		},
		"./performance": {
			"import": "./dist/performance/index.js",
			"require": "./dist/performance/index.js",
			"types": "./dist/performance/index.d.ts"
		},
		"./migrations": {
			"import": "./dist/migrations/index.js",
			"require": "./dist/migrations/index.js",
			"types": "./dist/migrations/index.d.ts"
		},
		"./monitoring": {
			"import": "./dist/monitoring/index.js",
			"require": "./dist/monitoring/index.js",
			"types": "./dist/monitoring/index.d.ts"
		},
		"./security": {
			"import": "./dist/security/index.js",
			"require": "./dist/security/index.js",
			"types": "./dist/security/index.d.ts"
		}
	}
}
